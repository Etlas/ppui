{
    "compilerOptions": {
        "outDir": "./publish/",
        "sourceMap": false,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "moduleResolution": "node", 
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "declaration": false,
        "lib": [
            "es5", 
            "dom"
        ]
    },
    "include": [
        "./src/**/*"
    ]
}